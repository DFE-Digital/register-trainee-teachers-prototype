
{% extends "_layouts/main.njk" %}

{% set pageHeading = "Page not found" %}

{% block pageTitle %}
  {{ pageHeading }} - {{serviceName}} - GOV.UK
{% endblock %}

{% block content %}

<div class="govuk-grid-row">

  <div class="govuk-grid-column-two-thirds">
    <h1 class="govuk-heading-l">{{ pageHeading }}</h1>

    <p class="govuk-body">
      Check the web address and try again.
    </p>

    <p class="govuk-body">
      If you continue to get this message, email the Register trainee teachers team at <a class="govuk-link" href="mailto:becomingateacher@digital.education.gov.uk?subject=Page%20not%20found">becomingateacher@digital.education.gov.uk</a>.
    </p>

    {{ govukButton({
      "text": "Return home",
      href: '/'
    }) }}

  </div>

</div>

{% endblock %}
