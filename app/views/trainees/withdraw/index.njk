{% extends "layouts/main.njk" %}

{% set primaryNavId = "SOMETHING" %}
{% set secondaryNavId = "about" %}

{% set title = "Withdraw trainee" %}
{% set caption = "Gladia Delmarre" %}

{% block beforeContent %}
{{ govukBackLink({
  text: "Back",
  href: "#"
}) }}
{% endblock %}

{% block content %}

{% include "_includes/notification-banner.njk" %}

<div class="govuk-grid-row">
  <div class="govuk-grid-column-two-thirds">

    {% include "_includes/page-heading.njk" %}

    <h2 class="govuk-heading-m">Before you start</h2>

    <p class="govuk-body">
      Make sure you have all the information related to the withdrawal of this trainee.
    </p>

    <h2 class="govuk-heading-m">How your answers help us</h2>

    <p class="govuk-body">
      These may help inform policy in the future and help increase trainee retention.
    </p>

    {{ govukButton({
      text: "Start",
      href: actions.next,
      isStartButton: true
    }) }}

    <p class="govuk-body">
      <a class="govuk-link" href="{{ actions.cancel }}">Cancel and return to record</a>
    </p>


  </div>
</div>

{% endblock %}
