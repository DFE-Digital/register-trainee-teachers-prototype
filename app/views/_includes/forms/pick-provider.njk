{% set providers = data.signedInProviders %}


{{providers | log}}

{% set providerItems = [] %}

{% for provider in providers %}
  {% set providerItems = providerItems | push({
    text: provider
  }) %}
{% endfor %}

{# {% set providerItems = providerItems | push({divider: 'or'}) %}
